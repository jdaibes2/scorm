    "_disableAccessibilityState": false,
    "title": "New Component Title",
    "displayTitle": "Vegetables, fruits, and disease",
    "body": "",
    "_id": "612f5290e963ae172898564c",
    "_parentId": "612f528ce963ae172898564b",
    "_type": "component",
    "_layout": "full",
    "_onScreen": {
